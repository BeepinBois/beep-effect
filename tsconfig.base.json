{
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": false,
    "moduleDetection": "force",
    "composite": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [],
    "isolatedModules": true,
    "sourceMap": true,
    "declarationMap": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": false,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "target": "ES2022",
    "module": "esnext",
    "incremental": true,
    "removeComments": false,
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ],
    "paths": {
      "@beep/common": [
        "./packages/common/src/index"
      ],
      "@beep/common/*": [
        "./packages/common/src/*"
      ],
      "@beep/common/test/*": [
        "./packages/common/test/*"
      ],
      "@beep/repo-config": [
        "./tooling/repo-config/src/index"
      ],
      "@beep/repo-config/*": [
        "./tooling/repo-config/src/*"
      ],
      "@beep/repo-config/test/*": [
        "./tooling/repo-config/test/*"
      ],
      "@beep/repo-scripts": [
        "./tooling/repo-scripts/src/index"
      ],
      "@beep/repo-scripts/*": [
        "./tooling/repo-scripts/src/*"
      ],
      "@beep/repo-scripts/test/*": [
        "./tooling/repo-scripts/test/*"
      ],
      "@beep/testkit": [
        "./tooling/testkit/src/index"
      ],
      "@beep/testkit/*": [
        "./tooling/testkit/src/*"
      ],
      "@beep/testkit/test/*": [
        "./tooling/testkit/test/*"
      ],
      "@beep/tooling-utils": [
        "./tooling/utils/src/index"
      ],
      "@beep/tooling-utils/*": [
        "./tooling/utils/src/*"
      ],
      "@beep/tooling-utils/test/*": [
        "./tooling/utils/test/*"
      ],
      "@beep/ui/*": [
        "./packages/ui/src/*"
      ],
      "@beep/ui/test/*": [
        "./packages/ui/src/test/*"
      ]
    }
  }
}
