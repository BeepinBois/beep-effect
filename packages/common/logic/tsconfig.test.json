{
  "extends": "../../../tsconfig.base.json",
  "include": ["test"],
  "references": [
    {
      "path": "tsconfig.src.json"
    },
    {
      "path": "../types"
    }
  ],
  "compilerOptions": {
    "types": ["node"],
    "tsBuildInfoFile": ".tsbuildinfo/test.tsbuildinfo",
    "rootDir": "test",
    "noEmit": true
  },
  "paths": {
    "@beep/logic": ["./src/index"],
    "@beep/logic/*": ["./src/*"],
    "@beep/types": ["../types/src/index"],
    "@beep/types/*": ["../types/src/*"]
  }
}
