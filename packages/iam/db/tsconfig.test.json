{
  "extends": "../../../tsconfig.base.json",
  "include": [
    "test"
  ],
  "references": [
    {
      "path": "tsconfig.src.json"
    },
    {
      "path": "../tables"
    }
  ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "tsBuildInfoFile": ".tsbuildinfo/test.tsbuildinfo",
    "rootDir": "test",
    "noEmit": true
  },
  "paths": {
    "@beep/iam-db": [
      "./src/index"
    ],
    "@beep/iam-db/*": [
      "./src/*"
    ],
    "@beep/iam-tables": [
      "../tables/src/index"
    ],
    "@beep/iam-tables/*": [
      "../tables/src/*"
    ]
  }
}
