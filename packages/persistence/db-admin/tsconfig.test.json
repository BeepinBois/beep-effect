{
  "extends": "../../../tsconfig.base.json",
  "include": ["test"],
  "references": [
    {
      "path": "tsconfig.src.json"
    },
    {
      "path": "../../shared/tables"
    },
    {
      "path": "../../iam/tables"
    },
    {
      "path": "../db-scope"
    }
  ],
  "compilerOptions": {
    "types": ["node"],
    "tsBuildInfoFile": ".tsbuildinfo/test.tsbuildinfo",
    "rootDir": "test",
    "noEmit": true
  },
  "paths": {
    "@beep/db-admin": ["./src/index"],
    "@beep/db-admin/*": ["./src/*"],
    "@beep/iam-tables": ["../../iam/tables/src/index"],
    "@beep/iam-tables/*": ["../../iam/tables/src/*"],
    "@beep/shared-tables": ["../../shared/tables/src/index"],
    "@beep/shared-tables/*": ["../../shared/tables/src/*"],
    "@beep/db-scope": ["../db-scope/src/index"],
    "@beep/db-scope/*": ["../db-scope/src/*"]
  }
}
