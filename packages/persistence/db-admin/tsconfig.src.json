{
  "extends": "../../../tsconfig.base.json",
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../../iam/tables"
    },
    {
      "path": "../../shared/tables"
    },
    {
      "path": "../db-scope"
    }
  ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "outDir": "build/src",
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@beep/db-admin/*": ["./src/*"],
      "@beep/iam-tables": ["../../iam/tables/src/index"],
      "@beep/iam-tables/*": ["../../iam/tables/src/*"],
      "@beep/shared-tables": ["../../shared/tables/src/index"],
      "@beep/shared-tables/*": ["../../shared/tables/src/*"],
      "@beep/db-scope": ["../db-scope/src/index"],
      "@beep/db-scope/*": ["../db-scope/src/*"]
    }
  }
}
