{
  "name": "@beep/shared",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A library of shared stuff",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BeepinBois/beep-effect.git",
    "directory": "packages/shared"
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "dev": "tsc -b tsconfig.build.json --watch",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "execute": "pnpm tsx ./src/execute.ts",
    "test": "vitest run",
    "clean": "rm -rf node_modules .next out dist build .turbo .tsbuildinfo",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@standard-schema/spec": "^1.0.0",
    "@types/node": "^24.2.1",
    "drizzle-orm": "^0.44.4",
    "effect": "^3.17.7",
    "mutative": "^1.2.0",
    "next": "^15.4.6",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "tsx": "^4.20.3"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
