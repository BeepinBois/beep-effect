{
  "name": "@beep/form-dsl",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A library for the form system DSL",
  "publishConfig": {
    "access": "restricted",
    "directory": "build"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./*": "./src/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BeepinBois/beep-effect.git",
    "directory": "packages/form/dsl"
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-cjs && pnpm build-annotate",
    "dev": "tsc -b tsconfig.build.json --watch",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "clean": "rm -rf .tsbuildinfo node_modules coverage .next out dist build .turbo",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "effect": "^3.17.7",
    "@beep/schema": "workspace:*",
    "@beep/utils": "workspace:*",
    "@beep/invariant": "workspace:*"
  },
  "devDependencies": {
    "@beep/types": "workspace:*"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
