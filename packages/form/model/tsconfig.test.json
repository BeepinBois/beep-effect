{
  "extends": "../../../tsconfig.base.json",
  "include": [
    "test"
  ],
  "references": [
    {
      "path": "tsconfig.src.json"
    },
    {
      "path": "../../common/types"
    },
    {
      "path": "../../common/invariant"
    },
    {
      "path": "../../common/schema"
    },
    {
      "path": "../../common/utils"
    }
  ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "tsBuildInfoFile": ".tsbuildinfo/test.tsbuildinfo",
    "rootDir": "test",
    "noEmit": true
  },
  "paths": {
    "@beep/form-model": [
      "./src/index"
    ],
    "@beep/form-model/*": [
      "./src/*"
    ],
    "@beep/types": [
      "../../common/types/src/index"
    ],
    "@beep/types/*": [
      "../../common/types/src/*"
    ],
    "@beep/invariant": [
      "../../common/invariant/src/index"
    ],
    "@beep/invariant/*": [
      "../../common/invariant/src/*"
    ],
    "@beep/schema": [
      "../../common/schema/src/index"
    ],
    "@beep/schema/*": [
      "../../common/schema/src/*"
    ],
    "@beep/utils": [
      "../../common/utils/src/index"
    ],
    "@beep/utils/*": [
      "../../common/utils/src/*"
    ]
  }
}
