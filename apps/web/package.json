    "@jsonforms/react": "^3.6.0",
    "@jsonforms/core": "^3.6.0",
    "@jsonforms/material-renderers": "^3.6.0",
    "json-logic-js": "^2.0.5",
    "@beep/invariant": "workspace:^",
    "@beep/schema": "workspace:^",
    "@beep/types": "workspace:^",
    "@beep/ui": "workspace:^",
    "@beep/utils": "workspace:^",
