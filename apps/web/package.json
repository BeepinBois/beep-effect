{
  "name": "@beep/web",
  "author": "TODOX",
  "version": "0.1.0",
  "type": "module",
  "description": "TODOX Website",
  "private": true,
  "scripts": {
    "dev": "pnpm dotenvx next dev --turbopack",
    "dev:https": "pnpm dotenvx next dev --turbopack --https",
    "start": "pnpm dotenvx next start",
    "build": "pnpm dotenvx next build --turbopack",
    "clean": "rm -rf node_modules .next out dist build .turbo .tsbuildinfo",
    "lint": "biome check .",
    "test": "vitest run",
    "lint:fix": "biome check . --write",
    "check": "tsc --noEmit",
    "execute": "pnpm dotenvx tsx ./execute.ts",
    "dotenvx": "dotenvx run -f ../../.env --"
  },
  "dependencies": {
