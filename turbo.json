{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalDependencies": [
    "**/.env",
    "**/.env.*"
  ],
  "globalPassThroughEnv": [
    "NODE_TLS_REJECT_UNAUTHORIZED",
    "AWS_*",
    "SST_*"
  ],
  "tasks": {
    "build": {
      "persistent": false,
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "!.next/cache/**"
      ]
    },
    "update-deps": {
      "dependsOn": [
        "^update-deps"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:generate": {
      "dependsOn": [
        "^db:generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:push": {
      "dependsOn": [
        "^db:push"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:migrate": {
      "dependsOn": [
        "^db:migrate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "db:studio": {
      "dependsOn": [
        "^db:studio"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint": {
      "cache": true,
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "lint:fix": {
      "cache": true,
      "dependsOn": [
        "^lint:fix"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "check": {
      "dependsOn": [
        "^check"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
